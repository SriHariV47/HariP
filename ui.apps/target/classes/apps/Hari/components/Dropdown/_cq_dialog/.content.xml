<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <Country
                        granite:class="cq-dialog-dropdown-showhide"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                        emptyText="select"
                        fieldLabel="Country"
                        name="./country">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            cq-dialog-dropdown-showhide-target=".drop"/>
                        <items jcr:primaryType="nt:unstructured">
                            <india
                                jcr:primaryType="nt:unstructured"
                                text="india"
                                value="india"/>
                            <us
                                jcr:primaryType="nt:unstructured"
                                text="us"
                                value="us"/>
                        </items>
                    </Country>
                    <india
                        granite:class="drop"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            showhidetargetvalue="india"/>
                        <items jcr:primaryType="nt:unstructured">
                            <Logo_x0020_Path
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="When checked, enables definition of Call-To-Actions. The linked page of the first Call-To-Action is used when populating title and description."
                                fieldLabel="Logo Path"
                                name="./logopath"/>
                            <Logo_x0020_Mobile_x0020_Image
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Logo Mobile Image"
                                name="./logomobileimage"/>
                            <Logo_x0020_Link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Logo Link"
                                name="./logolink"/>
                            <Enable_x0020_Switch
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./enableswitch"
                                text="every thing ok ?"/>
                        </items>
                    </india>
                    <us
                        granite:class="drop"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            showhidetargetvalue="us"/>
                        <items jcr:primaryType="nt:unstructured">
                            <pathfield
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                name="./Pathfield"/>
                            <firstname
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./ftname"/>
                            <checkbox
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./checkbox"
                                text="student ?"/>
                        </items>
                    </us>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
